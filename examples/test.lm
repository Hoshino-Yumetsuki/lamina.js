// Function definition
func add(a, b) {
  return a + b;
}

var result = add(10, 20);
print("add(10, 20) = " + result);

// Fibonacci function
func fib(n) {
  if (n <= 1) {
    return n;
  }
  return fib(n - 1) + fib(n - 2);
}

print("fib(10) = " + fib(10));

